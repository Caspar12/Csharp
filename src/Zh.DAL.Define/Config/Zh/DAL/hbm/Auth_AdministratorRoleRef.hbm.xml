<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Zh.DAL.Define.Entities.Auth_AdministratorRoleRef, Zh.DAL.Define" table="Auth_AdministratorRoleRef">
		<id name="ID" type="Guid" unsaved-value="00000000-0000-0000-0000-000000000000">
			<column name="ID" length="16" sql-type="uniqueidentifier" not-null="true" unique="true"/>
            <generator class="assigned" />        
		</id>
        <!-- 自增Id -->
		<property name="Identity" type="Int32" update="false" insert="false" >
			<column name="`Identity`" length="4" sql-type="int" not-null="true"/>
		</property>
        <!-- 权限Id -->
		<property name="AdministatorId" type="Int32"  >
			<column name="AdministatorId" length="4" sql-type="int" not-null="true"/>
		</property>
        <!-- 角色Id -->
		<property name="RoleId" type="Guid"  >
			<column name="RoleId" length="16" sql-type="uniqueidentifier" not-null="true"/>
		</property>
        <!-- 优先级 -->
		<property name="Priority" type="Int32"  >
			<column name="Priority" length="4" sql-type="int" not-null="true"/>
		</property>
        
         <!-- 权限角色表 -->
        <many-to-one update="false" insert="false" name="Auth_Role" class="Zh.DAL.Define.Entities.Auth_Role, Zh.DAL.Define">
			<column name="RoleId" length="16" sql-type="uniqueidentifier" not-null="true"/>
		</many-to-one>
         <!--  -->
        <many-to-one update="false" insert="false" name="dt_Administrator" class="Zh.DAL.Define.Entities.dt_Administrator, Zh.DAL.Define">
			<column name="AdministatorId" length="4" sql-type="int" not-null="true"/>
		</many-to-one>
        
	 
	</class>
</hibernate-mapping>
