<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Zh.DAL.Define.Entities.TalkPrice_Image, Zh.DAL.Define" table="TalkPrice_Image">
