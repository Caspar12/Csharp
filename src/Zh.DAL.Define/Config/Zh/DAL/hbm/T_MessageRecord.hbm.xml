<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Zh.DAL.Define.Entities.T_MessageRecord, Zh.DAL.Define" table="T_MessageRecord">
		<id name="id" type="Int32" unsaved-value="null">
			<column name="id" length="4" sql-type="int" not-null="true" unique="true"/>
            <generator class="native" />        
		</id>
        <!--  -->
		<property name="messageType" type="String"  >
			<column name="messageType" length="50" sql-type="nvarchar" not-null="false"/>
		</property>
        <!--  -->
		<property name="count" type="Int32"  >
			<column name="count" length="4" sql-type="int" not-null="false"/>
		</property>
        <!--  -->
		<property name="sendDate" type="DateTime"  >
			<column name="sendDate" length="8" sql-type="datetime" not-null="false"/>
		</property>
        <!--  -->
		<property name="shopid" type="String"  >
			<column name="shopid" length="50" sql-type="nvarchar" not-null="false"/>
		</property>
        <!--  -->
		<property name="messageContent" type="String"  >
			<column name="messageContent" length="500" sql-type="nvarchar" not-null="false"/>
		</property>
        
        
	 
	</class>
</hibernate-mapping>
