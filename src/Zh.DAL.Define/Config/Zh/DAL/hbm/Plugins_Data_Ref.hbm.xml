		<id name="ID" type="Int32" unsaved-value="null">
			<column name="ID" length="4" sql-type="int" not-null="true" unique="true"/>
            <generator class="native" />        
		</id>
        <!-- 第三方ID -->
		<property name="PluginsID" type="String"  >
			<column name="PluginsID" length="128" sql-type="nvarchar" not-null="true"/>
		</property>
        <!-- 分类ID -->
		<property name="CategoryID" type="String"  >
			<column name="CategoryID" length="128" sql-type="nvarchar" not-null="false"/>
		</property>
        <!-- 第三方数据ID -->
		<property name="PluginsItemID" type="String"  >
			<column name="PluginsItemID" length="128" sql-type="nvarchar" not-null="true"/>
		</property>
        <!-- 系统数据ID -->
		<property name="ItemID" type="String"  >
			<column name="ItemID" length="128" sql-type="nvarchar" not-null="true"/>
		</property>
        
        
	 
	</class>
</hibernate-mapping>
