<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Zh.DAL.Define.Entities.Com_Click, Zh.DAL.Define" table="Com_Click">
		<id name="ID" type="Guid" unsaved-value="00000000-0000-0000-0000-000000000000">
			<column name="ID" length="16" sql-type="uniqueidentifier" not-null="true" unique="true"/>
            <generator class="assigned" />        
		</id>
        <!-- 分类标识 -->
		<property name="Category" type="String"  >
			<column name="Category" length="32" sql-type="nvarchar" not-null="false"/>
		</property>
        <!-- 点击数量 -->
		<property name="ClickCount" type="Int32"  >
			<column name="ClickCount" length="4" sql-type="int" not-null="false"/>
		</property>
        
        
	 
	</class>
</hibernate-mapping>
