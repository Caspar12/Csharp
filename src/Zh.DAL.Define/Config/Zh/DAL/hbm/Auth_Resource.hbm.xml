<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Zh.DAL.Define.Entities.Auth_Resource, Zh.DAL.Define" table="Auth_Resource">
		<id name="ID" type="Guid" unsaved-value="00000000-0000-0000-0000-000000000000">
			<column name="ID" length="16" sql-type="uniqueidentifier" not-null="true" unique="true"/>
            <generator class="assigned" />        
		</id>
        <!-- 自增Id -->
		<property name="Identity" type="Int32" update="false" insert="false" >
			<column name="`Identity`" length="4" sql-type="int" not-null="true"/>
		</property>
        <!-- 名称 -->
		<property name="Name" type="String"  >
			<column name="Name" length="128" sql-type="nvarchar" not-null="true"/>
		</property>
        <!-- 描述 -->
		<property name="Description" type="String"  >
			<column name="Description" length="16" sql-type="ntext" not-null="false"/>
		</property>
        <!-- 排序 -->
		<property name="Sort" type="Int32"  >
			<column name="Sort" length="4" sql-type="int" not-null="true"/>
		</property>
        
        
		<bag name="Auth_PermissionResourceRef" inverse="true" lazy="true" cascade="all">
			<key column="ResourceId"/>
			<one-to-many class="Zh.DAL.Define.Entities.Auth_PermissionResourceRef, Zh.DAL.Define"/>
		</bag>
		<bag name="CWX_WeiXinApplication" inverse="true" lazy="true" cascade="all">
			<key column="AuthResourceId"/>
			<one-to-many class="Zh.DAL.Define.Entities.CWX_WeiXinApplication, Zh.DAL.Define"/>
		</bag>
	 
	</class>
</hibernate-mapping>
